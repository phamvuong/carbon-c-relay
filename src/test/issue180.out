listen
    linemode
        2003 proto tcp
        2003 proto udp
        /tmp/.s.carbon-c-relay.2003 proto unix
    ;

statistics
    submit every 60 seconds
    prefix with carbon.relays.test_hostname
    ;


match foo
    send to blackhole
    stop
    ;
match baz
    send to blackhole
    stop
    ;
match bar
    send to blackhole
    stop
    ;

